{
  "name": "petstore",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "npm run clean:tmp && parcel src/index.html -p 3000 --out-dir .tmp",
    "build": "npm run lint && npm run clean && npm run build:parcel",
    "lint": "npm run lint:js && npm run lint:css",
    "lint-fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js:fix": "eslint ./src/**/*.js --fix",
    "lint:css:fix": "stylelint ./src/**/*.css --fix",
    "lint:css": "stylelint --ignore-path .gitignore ./src/**/*.css",
    "lint:js": "eslint --ignore-path .gitignore -- ./src",
    "clean": "npm run clean:build",
    "clean:tmp": "rimraf ./.tmp",
    "clean:build": "rimraf ./build",
    "build:parcel": "parcel build src/index.html --out-dir build --no-cache"
  },
  "author": "titanishu",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "parcel-bundler": "^1.6.2",
    "postcss-modules": "^1.1.0",
    "prettier": "^1.11.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.1.1"
  },
  "dependencies": {
    "firebase": "^4.10.1",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "materialize-css": "^1.0.0-alpha.4",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-firebase": "^2.2.8",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  }
}
