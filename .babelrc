{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-es2015-arrow-functions",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
